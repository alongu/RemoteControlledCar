<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="guti.HomeCar" >
    <application
        android:configChanges="keyboardHidden|orientation"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="guti.HomeCar.MainActivity"
            android:screenOrientation="landscape"
            android:configChanges="keyboardHidden|orientation"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="guti.HomeCar.BTClass"
            android:label="@string/title_activity_btclass"
            android:parentActivityName="guti.HomeCar.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="guti.HomeCar.MainActivity" />
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>
